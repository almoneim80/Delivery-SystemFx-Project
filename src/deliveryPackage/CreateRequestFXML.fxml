<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CreateRequestFXMLController">
        <Label layoutX="100" layoutY="60"  fx:id="Titellabel" />

        <Label layoutX="400" layoutY="150" fx:id="createRequestUsernameLabel" text="رقم الجوال" />  
        <TextField layoutX="200" layoutY="150" fx:id="createRequestphoneField" promptText="رقم الجوال"/>
        
        <Label layoutX="400" layoutY="190" fx:id="createRequestCurrentLoLabel" text="الموقع الحالي" />  
        <TextField layoutX="200" layoutY="190" fx:id="createRequestCurrentLoField" promptText="الموقع الحالي"/>
        
        
        <Label layoutX="400" layoutY="230" fx:id="createRequestGoalLoLabel" text="الموقع المراد" />  
        <TextField layoutX="200" layoutY="230" fx:id="createRequestGoalLoField" promptText="الموقع المراد"/>
        
        <Label layoutX="400" layoutY="270" fx:id="createRequestDriverLabel" text="السائق" />  
        <ComboBox layoutX="200" layoutY="270" fx:id="comb" promptText="أختر سائق"  />
        
        <Button layoutX="270" layoutY="320" text="انشــاء طلــب" onAction="#CreateRequestBTNAction" fx:id="CreateBTN" />
        <Button layoutX="200" layoutY="320" text="طباعــة" onAction="#PrintRequestAction" fx:id="PrintBTN" />
        
        <Label layoutX="400" layoutY="360" fx:id="label" />
</AnchorPane>
